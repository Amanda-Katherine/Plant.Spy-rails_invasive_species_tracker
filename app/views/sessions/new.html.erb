<h1>Login</h1>

<%= form_for :user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username%>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Login "%>
<% end %>

<%= button_to "Login with Facebook", '/auth/facebook'%>
<%= button_to "Login with Github", '/auth/github'%>
